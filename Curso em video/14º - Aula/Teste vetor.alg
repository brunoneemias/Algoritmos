algoritmo "teste vetor"
// Função :
// Autor :
// Data : 11/02/2018
// Seção de Declarações 
var
   V: vetor[1..6] de inteiro
   c: inteiro
inicio
      Para c <- 1 ate 6 faca
           Escreva("Digite o ",c,"º valor: ")
           leia(v[c])
      fimpara
      
      Para c <- 1 ate 6 faca
           Escreva("[",V[c], " ]")
      fimpara
fimalgoritmo
